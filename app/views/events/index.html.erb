<% content_for :body_content_left do %>
  <ul id="events">
    <% @events.each do |event| %>
      <li>
        <%= link_to event.name, event_path(event) %>
      </li>
    <% end %>
  </ul>
<% end %>


<div id="calendar">
  <%= raw event_calendar %>
</div>

<%= render :partial => "/shared/content_page" %>
